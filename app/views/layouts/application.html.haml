!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title KellyeGreene.com
    = csrf_meta_tags
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
    %script{:crossorigin => "anonymous", :integrity => "sha256-eGE6blurk5sHj+rmkfsGYeKyZx3M4bG+ZlFyA7Kns7E=", :src => "https://code.jquery.com/ui/1.12.0/jquery-ui.min.js"}
    %link{:href => "//code.jquery.com/ui/1.12.0/themes/base/jquery-ui.css", :rel => "stylesheet"}/
  %body
    .osx
      .top-bar
        %ul.top-bar__menu
          %li#apple_menu
            = link_to do
              = fa_icon "apple"
          %li#active_program
            = link_to "Terminal"
      .desktop
        = yield
      .dock
        %ul
          %li.dock__item#dock_item_terminal.active{ onClick: "dockItemClick(this)"}
            %a{ title: "Terminal" } 
              = image_tag "terminal.png"
          %li.dock__item#dock_item_bio
            %a{ title: "Bio" } 
              = image_tag "bio.png"
          %li.dock__item#dock_item_portfolio
            %a{ title: "Portfolio" } 
              = image_tag "portfolio.png"
          %li.dock__item#dock_item_contact
            %a{ title: "Contact" } 
              = image_tag "contact.png"
        .dock__background



